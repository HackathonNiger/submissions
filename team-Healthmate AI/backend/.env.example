# Environment Variables for Healthmate AI Backend
# Copy this file to .env and fill in your actual values

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Azure Services Configuration (Optional - for fallback)
AZURE_HEALTH_INSIGHTS_ENDPOINT=your_azure_health_insights_endpoint
AZURE_HEALTH_INSIGHTS_KEY=your_azure_health_insights_key
AZURE_TRANSLATOR_KEY=your_azure_translator_key
AZURE_TRANSLATOR_ENDPOINT=your_azure_translator_endpoint
AZURE_TRANSLATOR_REGION=your_azure_region

# Supabase Database Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# Example Supabase values (replace with your actual values):
# SUPABASE_URL=https://your-project.supabase.co
# SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Flask Configuration
PORT=5000
FLASK_ENV=development